{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "minify-script": {},
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
